@model IEnumerable<GuestResponse>
@{
    Layout = null;
}

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta name="viewport" content="width=device-width" />
        <title></title>
    </head>
    <body>
        <h2>Here is the list of people attending the party</h2>
        <table>
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                </tr>
            </thead>
            <tbody>
            @foreach (GuestResponse guest in Model)
            {
                <tr>
                    <td>@guest.Name</td>
                    <td>@guest.Email</td>
                    <td>@guest.Phone</td>
                </tr>
            }
        </tbody>
        </table>
    </body>
</html>